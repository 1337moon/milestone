let age = 25;
let category;

if (age >= 0 && age <= 2) {
  category = "Infant";
} else if (age >= 3 && age <= 12) {
  category = "Child";
} else if (age >= 13 && age <= 17) {
  category = "Teenager";
} else if (age >= 18 && age <= 59) {
  category = "Adult";
} else if (age >= 60 && age <= 120) {
  category = "Senior Citizen";
} else {
  category = "Die soon";
}

console.log(`Category: ${category}`);
